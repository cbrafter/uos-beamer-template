\mode<presentation>
\RequirePackage{color}
\RequirePackage{hyperref}
% Settings
\definecolor{uosblue}{rgb}{0, 0.3516, 0.5273}

%Title page object colors
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{title}{fg=white}
\setbeamercolor*{subtitle}{fg=white}
\setbeamercolor*{title separator}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{institute}{fg=white}

% Frame Header Color
\setbeamercolor{palette quaternary}{fg=white,bg=uosblue}
\setbeamercolor{titlelike}{parent=palette quaternary}
\setbeamercolor{block title}{fg=white, bg=uosblue}

% Item and bullets
\setbeamercolor{item}{fg=uosblue}
\setbeamercolor{footnote mark}{fg=uosblue}

\mode<all>